#include<iostream>
using namespace std;
int main()
{
    int n;
    cin>>n;
    int arr[n];
    for(int k=0;k<n;k++)
    {
    cin>>arr[n];
    }
        for (int i=0;i<(n/2);i++)
        {
          int temp;
          temp=arr[i];
          arr[i]=arr[n-(i+1)];
          arr[n-(i+1)]=temp;
          cout<<arr[i];
        }


}
